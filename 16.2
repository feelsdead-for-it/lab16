#include <iostream>
using namespace std;
int main()
{
    
    int n, i, s, r, counter;
    cin >> n;
    int* a = new int [n];
    
    for (i = 0; i < n; i++)
    {
        cin >> a[i];
    }
    
    s = 0;
    
    for (i = 0; i < n; i++)
    {
        counter = 0;
        for (r = 0; r < n; r++)
        {
            if (a[i] == a[r])
            {
                counter++;
            }
        }
        if (counter == 1 || counter > 2)
        {
            a[s] = a[i];
            s++;
        }
    }
    
    
    for (i = 0; i < s; i++)
    {
        cout << a[i] << " ";
    }
    cout << endl;
    cout << s;
    
   
}
